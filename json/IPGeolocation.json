{
  "openapi": "3.0.3",
  "info": {
    "title": "IP2Location.io IP Geolocation API",
    "description": "IP2Location.io IP Geolocation API provides RESTful API to obtain visitorsâ€™ geolocation information such as country, region, city, latitude & longitude, ZIP code, time zone, ASN, ISP, domain, net speed, IDD code, area code, weather station data, MNC, MCC, mobile brand, elevation, usage type, address type, advertisement category and proxy data with an IP address.",
    "version": "1",
    "contact": {
		"name": "IP2Location.io",
		"url": "https://github.com/ip2location/ip2location-io-openapi"
	}
  },
  "servers": [
    {
      "url": "https://api.ip2location.io"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "summary": "IP Geolocation",
        "description": "Geolocate user's location information via IP address",
        "operationId": "ipGeolocation",
        "parameters": [
          {
            "name": "key",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "(required) IP2Location.io API key."
          },
          {
            "name": "ip",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "example": "8.8.8.8"
            },
            "description": "(required) IP address (IPv4 or IPv6) for reverse IP location lookup purposes. If not present, the server IP address will be used for the location lookup."
          },
          {
            "name": "format",
            "in": "query",
            "schema": {
              "enum": [
                "json",
                "xml"
              ],
              "type": "string"
            },
            "description": "(optional) Format of the response message. Default is json."
          },
          {
            "name": "lang",
            "in": "query",
            "schema": {
              "enum": [
                "ar",
                "cs",
                "da",
                "de",
                "en",
                "es",
                "et",
                "fi",
                "fr",
                "ga",
                "it",
                "ja",
                "ko",
                "ms",
                "nl",
                "pt",
                "ru",
                "sv",
                "tr",
                "vi",
                "zh-cn",
                "zh-tw"
              ],
              "type": "string"
            },
            "description": "(optional) Translation information(ISO639-1). The translation is only applicable for continent, country, region and city name. Note: This parameter is only available for Plus and Security plan only."
          }
        ],
